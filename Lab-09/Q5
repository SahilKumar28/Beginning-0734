#include<stdio.h>
#include<string.h>
void reverse(char a[10]){
 int i,len=strlen(a),j=0;
 char reversed_string[len];
 for(i=len-1;i>=0;i--){
 reversed_string[j]=a[i];
 j++;
 }
 for(i=0;i<len;i++){
 printf("%c",reversed_string[i]);
 }

}
int main(){
   char b[10];
   printf("Enter the string:");
   gets(b);
   reverse(b);
}
