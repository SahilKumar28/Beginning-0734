#include<stdio.h>
int main(){
	int k,a=0,e=0,i=0,o=0,u=0;
	char str[100];
	printf("Enter the string:");
	gets(str);
	for(k=0;str[k]!='\0';k++){
		if(str[k]=='A'||str[k]=='a') a++;
		else if(str[k]=='E'||str[k]=='e') e++;
		else if(str[k]=='I'||str[k]=='i') i++;
		else if(str[k]=='O'||str[k]=='o') o++;
		else if(str[k]=='U'||str[k]=='u') u++;
	}
	printf("A:%d\n",a);
	printf("E:%d\n",e);
	printf("I:%d\n",i);
	printf("O:%d\n",o);
	printf("U:%d\n",u);
}
